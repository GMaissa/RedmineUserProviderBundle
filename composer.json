{
    "name": "gmaissa/redmine-user-provider-bundle",
    "type": "symfony-bundle",
    "description": "A bundle to use Redmine as a user provider for Symfony projects.",
    "keywords": ["security", "user provider", "redmine"],
    "homepage": "https://github.com/GMaissa/RedmineUserProviderBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume MaÃ¯ssa",
            "email": "pro.g@maissa.fr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0",
        "symfony/framework-bundle": "^3.0",
        "kbsali/redmine-api": "^1.5",
        "doctrine/collections": "^1.4",
        "symfony/yaml": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/phpunit-bridge": "3.3.*@dev",
        "squizlabs/php_codesniffer": "^2.8",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/oauth-server-bundle": "^1.5"
    },
    "suggest": {
        "friendsofsymfony/oauth-server-bundle": "Manage OAuth2 authentication for API"
    },
    "autoload":{
        "psr-4":{
            "GMaissa\\RedmineUserProviderBundle\\": "src"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "GMaissa\\RedmineUserProviderBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
